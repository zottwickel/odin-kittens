<%= form_for(@kitten) do |f| %>
	<% if @kitten.errors.any? %>
	<span>The form contains <%= pluralize(@kitten.errors.count, "error") %>.</span><br>
	<span>You should probably deal with your cat problem(bahaha)</span>
	<% end %><br>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :age %>
	<%= f.number_field :age, in: 0..40, step: 1 %>
	<%= f.label :cuteness %>
	<%= f.number_field :cuteness, in: 1..10, step: 1 %>
	<%= f.label :softness %>
	<%= f.number_field :softness, in: 1..10, step: 1 %>
	<%= f.submit "SUBMIT" %>
<% end %>